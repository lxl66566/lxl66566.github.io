{
  "[html]": {
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.formatOnSave": true
  },
  "codeium.enableConfig": {
    "markdown": false,
    "vue": false,
    "*": true
  },
  "oxc.enable": true,
  "Codegeex.DisabledFor": {
    "markdown": "true"
  },
  "Lingma.cloudModelEnable": false,
  "Lingma.cloudModelAutoTriggerDisplayRule": false,
  "Lingma.localMachineModel": false,
  "autocorrect.enable": false,
  "cSpell.language": "en-US",
  "cSpell.enableFiletypes": ["bat", "ini", "lua"],
  "markdownlint.config": {
    "default": true,
    "MD024": false,
    "MD031": false,
    "MD033": false,
    "MD036": false,
    "MD040": false,
    "MD056": false
  },
  "continue.enableTabAutocomplete": false,
  "supermaven.enable": {
    "markdown": false
  },
  "cursor.aipreview.enabled": true,
  "cursor.cpp.disabledLanguages": ["markdown", "html", "vue"]
}
